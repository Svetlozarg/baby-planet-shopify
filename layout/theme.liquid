<!doctype html>
<!--[if lt IE 7]><html class="no-js lt-ie9 lt-ie8 lt-ie7" lang="en"> <![endif]-->
<!--[if IE 7]><html class="no-js lt-ie9 lt-ie8" lang="en"> <![endif]-->
<!--[if IE 8]><html class="no-js lt-ie9" lang="en"> <![endif]-->
<!--[if IE 9 ]><html class="ie9 no-js"> <![endif]-->
<!--[if (gt IE 9)|!(IE)]><!--> <html class="no-js"> <!--<![endif]-->
<head>

  <!-- Basic page needs ================================================== -->
  <meta charset="utf-8">
  <!--[if IE]><meta http-equiv='X-UA-Compatible' content='IE=edge,chrome=1'><![endif]-->

  {% if settings.favicon_enable %}
  <link rel="shortcut icon" href="{{ 'favicon.png' | asset_url }}" type="image/png" />
  {% endif %}

  <!-- Title and description ================================================== -->
  <title>
  {{ page_title }}{% if current_tags %}{% assign meta_tags = current_tags | join: ', ' %} &ndash; {{ 'general.meta.tags' | t: tags: meta_tags }}{% endif %}{% if current_page != 1 %} &ndash; {{ 'general.meta.page' | t: page: current_page }}{% endif %}{% unless page_title contains shop.name %} &ndash; {{ shop.name }}{% endunless %}
  </title>
  {% if page_description %}
  <meta name="description" content="{{ page_description | escape }}">
  {% endif %}
  <!-- Social meta ================================================== -->
  {% include 'social-meta-tags' %}
  <!-- Helpers ================================================== -->
  <link rel="canonical" href="{{ canonical_url }}">
  <meta name="viewport" content="width=device-width,initial-scale=1">
  <meta name="theme-color" content="{{ settings.color_primary }}">

  <!-- CSS ================================================== -->
  {{ 'timber.scss.css' | asset_url | stylesheet_tag }}
  {{ 'fonts.min.css' | asset_url | stylesheet_tag }}
  {{ 'style.css' | asset_url | stylesheet_tag }}


  {% if settings.base_font contains 'Google' %}
  <link rel="stylesheet" type="text/css" href="//fonts.googleapis.com/css?family={{ settings.base_font | remove: 'Google+' }}:300,300italic,400,600,400italic,600italic,700,700italic,800,800italic">
  {% endif %}
  {% if settings.header_font contains 'Google' %}
  <link rel="stylesheet" type="text/css" href="//fonts.googleapis.com/css?family={{ settings.header_font | remove: 'Google+' }}:300,300italic,400,600,400italic,600italic,700,700italic,800,800italic">
  {% endif %}
  {% if settings.other_font contains 'Google' %}
  <link rel="stylesheet" type="text/css" href="//fonts.googleapis.com/css?family={{ settings.other_font | remove: 'Google+' }}:300,300italic,400,600,400italic,600italic,700,700italic,800,800italic">
  {% endif %}


   <!-- Header hook for plugins ================================================== -->
  {{ content_for_header }}
  {% include 'oldIE-js' %}
  {{ 'header.js' | asset_url | script_tag }}

  {% if template contains 'customers' %}
  {{ 'shopify_common.js' | shopify_asset_url | script_tag }}
  {% endif %}

  <style>
    /* Header Type 2 - Improved Responsive Layout */
    .header-type-2 .site-header__menubar {
      display: flex;
      align-items: center;
      justify-content: space-between;
      padding: 15px 5%;
      max-width: 100%;
      position: relative;
      flex-wrap: wrap;
      min-height: 70px;
    }

    .header-type-2 .site-header__logo {
      padding-left: 0;
      flex: 0 0 auto;
      order: 1;
      z-index: 2;
    }

    .header-type-2 .site-header__logo img {
      max-height: 50px;
      height: auto;
      width: auto;
    }

    .header-type-2 .menubar-section {
      flex: 1 1 auto;
      order: 2;
      text-align: center;
      padding: 0 20px;
    }

    .header-type-2 .header-right-section {
      padding-right: 0;
      flex: 0 0 auto;
      order: 3;
      display: flex;
      align-items: center;
      gap: 15px;
    }

    /* Mobile Menu Button - Now on the right */
    .header-type-2 .mobile-menu-trigger {
      display: none;
      background: none;
      border: none;
      cursor: pointer;
      padding: 10px;
      font-size: 22px;
      color: #7b6b8a;
      z-index: 3;
      transition: color 0.3s ease;
      border-radius: 8px;
      flex-shrink: 0;
    }

    .header-type-2 .mobile-menu-trigger:hover,
    .header-type-2 .mobile-menu-trigger.active {
      color: #ff6b9d;
      background: rgba(255, 107, 157, 0.1);
    }

    /* Large Desktop Screens */
    @media only screen and (min-width: 1200px) {
      .header-type-2 .site-header__menubar {
        padding: 20px 3%;
      }

      .header-type-2 .site-header__logo img {
        max-height: 60px;
      }
    }

    /* Tablet and Medium Screens - Lower threshold to 967px to match existing mobile menu logic */
    @media only screen and (max-width: 967px) {
      .header-type-2 .site-header__menubar {
        padding: 15px 4%;
      }

      .header-type-2 .menubar-section {
        display: none !important; /* Hide desktop navigation on tablets and mobile */
      }

      .header-type-2 .mobile-menu-trigger {
        display: flex !important;
        align-items: center;
        justify-content: center;
      }
    }

    /* Additional mobile layout fixes for screens below 967px */
    @media only screen and (max-width: 767px) {
      .header-type-2 .site-header__menubar {
        padding: 12px 15px;
        min-height: 60px;
        gap: 15px;
      }

      .header-type-2 .site-header__logo {
        order: 1; /* Logo on left */
        flex: 1 1 auto;
        text-align: left;
      }

      .header-type-2 .site-header__logo img {
        max-height: 45px;
      }

      .header-type-2 .mobile-menu-trigger {
        min-width: 44px;
        height: 44px;
      }

      .header-type-2 .header-right-section {
        order: 2;
        gap: 10px;
      }
    }

    /* Very Small Mobile Screens - 567px and below */
    @media only screen and (max-width: 567px) {
      .header-type-2 .site-header__menubar {
        padding: 10px 15px;
        gap: 10px;
        min-height: 55px;
        position: relative;
        display: flex;
        justify-content: space-between;
        align-items: center;
        flex-wrap: nowrap;
      }

      .header-type-2 .site-header__logo {
        order: 1;
        text-align: left;
        flex: 1 1 auto;
        margin: 0;
        padding: 0;
        max-width: calc(100% - 120px); /* Reserve space for right section */
      }

      .header-type-2 .site-header__logo img {
        max-height: 35px !important;
        height: auto;
        width: auto;
      }

      .header-type-2 .mobile-menu-trigger {
        padding: 8px;
        margin: 0;
        min-width: 40px;
        height: 40px;
        font-size: 18px;
      }

      .header-type-2 .header-right-section {
        order: 2;
        flex: 0 0 auto;
        gap: 8px;
        min-width: 100px;
        justify-content: flex-end;
      }

      /* Override any conflicting flexbox properties */
      .header-type-2 .menubar-section {
        display: none !important;
      }
    }

    /* Small Mobile Screens */
    @media only screen and (max-width: 480px) {
      .header-type-2 .site-header__menubar {
        min-height: 55px;
      }

      .header-type-2 .site-header__logo img {
        max-height: 35px;
      }
    }

    /* Ensure proper z-index for dropdowns */
    .header-type-2 .site-header {
      position: relative;
      z-index: 100;
    }

            /* Disable sticky header - header stays at top */
    .header-sticky {
      position: static !important;
    }

    .header-sticky.is-sticky {
      position: static !important;
      top: auto !important;
      left: auto !important;
      right: auto !important;
      z-index: auto !important;
      width: auto !important;
      height: auto !important;
    }

    /* Remove any sticky-specific styling */
    .header-sticky.is-sticky .header-type-2 .site-header__menubar,
    .header-sticky.is-sticky .header-type-2 .site-header__logo img,
    .header-sticky.is-sticky .header-type-2 .mobile-menu-trigger {
      /* Reset to inherit normal styling */
      min-height: inherit;
      padding: inherit;
      background: inherit;
      backdrop-filter: none;
      border-bottom: none;
      box-shadow: none;
      max-height: inherit;
      transition: none;
    }

    /* Header background */
    .header-type-2 .site-header {
      background: #fff;
    }

    .header-type-2 .site-header__menubar {
      background: #fff;
    }

            /* Mobile Menu Enhancements */
    .cbp-spmenu {
      background: linear-gradient(135deg, #fff 0%, #fef9ff 100%);
      z-index: 10000;
      overflow-y: auto;
      padding-bottom: 20px;
    }

    .cbp-spmenu .close-menu {
      background: #f0e8f3;
      color: #7b6b8a;
      border-radius: 50%;
      width: 40px;
      height: 40px;
      display: flex;
      align-items: center;
      justify-content: center;
      margin: 15px 20px 0 auto;
      cursor: pointer;
      transition: all 0.3s ease;
      border: none;
      font-size: 18px;
    }

    .cbp-spmenu .close-menu:hover {
      background: #ff6b9d;
      color: #fff;
    }

    /* Mobile Header Actions Styling */
    .mobile-header-actions {
      padding: 0 20px 20px;
      border-bottom: 2px solid #f0e8f3;
      margin-bottom: 20px;
    }

    .mobile-header-actions h3 {
      color: #7b6b8a;
      font-size: 14px;
      font-weight: 600;
      text-transform: uppercase;
      letter-spacing: 1px;
      margin: 0 0 15px 0;
      padding: 10px 0;
    }

    .mobile-action-item {
      margin-bottom: 12px;
    }

    .mobile-action-link {
      display: flex;
      align-items: center;
      padding: 12px 0;
      color: #7b6b8a;
      text-decoration: none;
      font-size: 16px;
      font-weight: 500;
      transition: color 0.3s ease;
      border-bottom: 1px solid #f5f0f7;
    }

    .mobile-action-link:hover {
      color: #ff6b9d;
    }

    .mobile-action-link i {
      margin-right: 12px;
      width: 20px;
      color: #b8a8c8;
      font-size: 16px;
    }

    .mobile-action-link:hover i {
      color: #ff6b9d;
    }

    /* Mobile Search */
    .mobile-search-toggle {
      display: flex;
      align-items: center;
      padding: 12px 0;
      color: #7b6b8a;
      cursor: pointer;
      font-size: 16px;
      font-weight: 500;
      transition: color 0.3s ease;
      border-bottom: 1px solid #f5f0f7;
    }

    .mobile-search-toggle:hover {
      color: #ff6b9d;
    }

    .mobile-search-toggle i {
      margin-right: 12px;
      width: 20px;
      color: #b8a8c8;
      font-size: 16px;
    }

    .mobile-search-toggle:hover i {
      color: #ff6b9d;
    }

    .mobile-search-form {
      padding: 15px 0;
      background: #fefbff;
      border-radius: 8px;
      margin-top: 10px;
    }

    .mobile-search-input-wrapper {
      position: relative;
      display: flex;
    }

    .mobile-search-input-wrapper input {
      flex: 1;
      padding: 12px 15px;
      border: 2px solid #f0e8f3;
      border-radius: 25px 0 0 25px;
      font-size: 14px;
      background: #fff;
      color: #7b6b8a;
      outline: none;
    }

    .mobile-search-input-wrapper input:focus {
      border-color: #ff6b9d;
    }

    .mobile-search-input-wrapper button {
      padding: 12px 18px;
      background: linear-gradient(135deg, #ff6b9d 0%, #ffa8cc 100%);
      color: #fff;
      border: none;
      border-radius: 0 25px 25px 0;
      cursor: pointer;
      transition: all 0.3s ease;
    }

    .mobile-search-input-wrapper button:hover {
      background: linear-gradient(135deg, #ff4d8a 0%, #ff87b3 100%);
    }

    /* Mobile User Menu */
    .mobile-user-header {
      display: flex;
      align-items: center;
      padding: 12px 0;
      color: #7b6b8a;
      font-size: 16px;
      font-weight: 500;
      border-bottom: 1px solid #f5f0f7;
    }

    .mobile-user-header i {
      margin-right: 12px;
      width: 20px;
      color: #b8a8c8;
      font-size: 16px;
    }

    .mobile-user-links {
      list-style: none;
      margin: 0;
      padding: 0;
    }

    .mobile-user-links li {
      margin: 0;
    }

    .mobile-user-links a {
      display: flex;
      align-items: center;
      padding: 10px 0 10px 20px;
      color: #9b8ba8;
      text-decoration: none;
      font-size: 14px;
      transition: color 0.3s ease;
    }

    .mobile-user-links a:hover {
      color: #ff6b9d;
    }

    .mobile-user-links a i {
      margin-right: 10px;
      width: 16px;
      color: #c8b8d8;
      font-size: 14px;
    }

    .mobile-user-links a:hover i {
      color: #ff6b9d;
    }

    .cbp-spmenu .gf-menu-device-container {
      padding: 0 20px;
      display: block !important;
      visibility: visible !important;
    }

    .cbp-spmenu .site-nav {
      list-style: none;
      padding: 0;
      margin: 0;
      display: block !important;
      visibility: visible !important;
      height: auto !important;
    }

    .cbp-spmenu .site-nav > li {
      border-bottom: 1px solid #f0e8f3;
      display: block !important;
      width: 100% !important;
    }

    .cbp-spmenu .site-nav > li > a {
      display: block !important;
      padding: 15px 0;
      color: #7b6b8a;
      text-decoration: none;
      font-size: 16px;
      font-weight: 500;
      transition: color 0.3s ease;
    }

    .cbp-spmenu .site-nav > li > a:hover,
    .cbp-spmenu .site-nav > li > a.current {
      color: #ff6b9d;
    }

    /* Override any hiding styles for mobile menu */
    @media only screen and (max-width: 967px) {
      .gf-menu-device-container .site-nav {
        display: block !important;
        overflow: visible !important;
        width: 100% !important;
        visibility: visible !important;
        height: auto !important;
      }

      .gf-menu-device-container .site-nav li {
        width: 100% !important;
        display: block !important;
      }

      .gf-menu-device-container .site-nav.gf-menu.clicked {
        visibility: visible !important;
        height: auto !important;
        display: block !important;
      }

      .cbp-spmenu .gf-menu-device-wrapper {
        display: block !important;
      }
    }

    /* Fix mobile menu to overlay from right instead of push */
    @media only screen and (max-width: 967px) {
      .cbp-spmenu-push-toleft {
        right: 0 !important;
        overflow: visible !important;
        position: static !important;
        width: auto !important;
      }

      .cbp-spmenu-right {
        position: fixed;
        right: -280px;
        width: 280px;
        height: 100vh;
        top: 0;
        z-index: 10000;
        transition: right 0.3s ease;
      }

      .cbp-spmenu-right.cbp-spmenu-open {
        right: 0;
        box-shadow: -2px 0 10px rgba(0, 0, 0, 0.1);
      }

      /* Add backdrop */
      body.cbp-spmenu-push-toleft::before {
        content: '';
        position: fixed;
        top: 0;
        left: 0;
        right: 0;
        bottom: 0;
        background: rgba(0, 0, 0, 0.5);
        z-index: 9999;
        opacity: 0;
        visibility: hidden;
        transition: all 0.3s ease;
      }

      body.cbp-spmenu-push-toleft.menu-open::before {
        opacity: 1;
        visibility: visible;
      }
    }

    /* Improve mobile menu accessibility */
    .mobile-menu-trigger:focus {
      outline: 2px solid #ff6b9d;
      outline-offset: 2px;
    }

    .sr-only {
      position: absolute !important;
      width: 1px !important;
      height: 1px !important;
      padding: 0 !important;
      margin: -1px !important;
      overflow: hidden !important;
      clip: rect(0, 0, 0, 0) !important;
      white-space: nowrap !important;
      border: 0 !important;
    }

    /* Prevent horizontal scroll on mobile */
    @media only screen and (max-width: 767px) {
      .wrapper-container {
        overflow-x: hidden;
      }
    }
  </style>
</head>

<body id="{{ page_title | handle }}" class="{% if customer %}customer-logged-in {% endif %}template-{{ template | replace: '.', ' ' | truncatewords: 1, '' | handle }}" >
<nav class="cbp-spmenu cbp-spmenu-vertical cbp-spmenu-right" id="cbp-spmenu-s1">
    <div class="gf-menu-device-wrapper">
      <div class="close-menu">Ã—</div>

      <!-- Mobile Header Actions -->
      <div class="mobile-header-actions">
        <h3>Actions</h3>

        <!-- Search -->
        <div class="mobile-action-item">
          <div class="mobile-search-toggle">
            <i class="fa fa-search"></i>
            <span>{{ 'general.search.title' | t | default: 'Search' }}</span>
          </div>
          <div class="mobile-search-form" style="display: none;">
            <form action="/search" method="get" role="search">
              {% if settings.show_search_prt %}
              <input type="hidden" name="type" value="product">
              {% endif %}
              <div class="mobile-search-input-wrapper">
                <input type="search"
                       name="q"
                       value="{{ search.terms | escape }}"
                       placeholder="ðŸ¼ {{ 'general.search.placeholder' | t | default: 'Search for baby products...' }}"
                       aria-label="{{ 'general.search.placeholder' | t }}">
                <button type="submit">
                  <i class="fa fa-search"></i>
                </button>
              </div>
            </form>
          </div>
        </div>

        <!-- User Account -->
        {% if shop.customer_accounts_enabled %}
        <div class="mobile-action-item">
          {% if customer %}
            <div class="mobile-user-menu">
              <div class="mobile-user-header">
                <i class="fa fa-user"></i>
                <span>{{ customer.first_name | default: 'Account' }}</span>
              </div>
              <ul class="mobile-user-links">
                <li><a href="/account"><i class="fa fa-user"></i> {{ 'layout.customer.account' | t }}</a></li>
                <li><a href="/account/addresses"><i class="fa fa-address-book-o"></i> {{ 'customer.addresses.title' | t }}</a></li>
                {% if settings.enable_wishlist %}
                <li><a href="/pages/wishlist"><i class="fa fa-heart"></i> {{ 'products.wishlist.wishlist_name' | t }}</a></li>
                {% endif %}
                <li>{{ 'layout.customer.log_out' | t | customer_logout_link | replace: '<a', '<a class="logout-link"' | replace: '>', '><i class="fa fa-sign-out"></i>' }}</li>
              </ul>
            </div>
          {% else %}
            <a href="/account/login" class="mobile-action-link">
              <i class="fa fa-sign-in"></i>
              <span>{{ 'layout.customer.log_in' | t }}</span>
            </a>
          {% endif %}
        </div>
        {% endif %}

        <!-- Wishlist -->
        {% if settings.enable_wishlist %}
        <div class="mobile-action-item">
          <a href="/pages/wishlist" class="mobile-action-link">
            <i class="fa fa-heart-o"></i>
            <span>{{ 'products.wishlist.wishlist_name' | t }}</span>
          </a>
        </div>
        {% endif %}
      </div>

      <!-- Navigation Menu -->
      <div class="gf-menu-device-container"></div>
    </div>
  </nav>
<div class="wrapper-container">
    <div class="header-type-2">
      {% section 'top-bar-type-2' %}

      {% include 'search-bar-type-2' %}

      <header class="site-header">
        <div class="header-sticky">
          <div id="header-landing" class="sticky-animate">
            <div class="site-header__menubar">
              {% include 'logo' %}
              <div class="menubar-section order-header">
                <div class="desktop-megamenu">
                  {% section 'navigation' %}
                </div>
              </div>
              <div class="header-right-section">
                {% section 'header-model-2' %}
                <button type="button" class="mobile-menu-trigger" id="showRightPush">
                  <i class="fa fa-bars" aria-hidden="true"></i>
                  <span class="sr-only">{{ 'general.accessibility.menu' | t | default: 'Menu' }}</span>
                </button>
              </div>
            </div>
          </div>
        </div>
      </header>
    </div>
  </div>
    {% if settings.preloader_enable %}<div class="se-pre-con"></div> {% endif %}
    <div id="PageContainer"></div>
    <div class="quick-view"></div>

   {% if settings.use_breadcrumb %}
    {% include 'breadcrumb' %}
    {% endif %}
  {% if template contains 'index'  %}
  {% if settings.show_slideshow  %}
  {% include 'slider-owl' %}
  <div class="dt-sc-hr-invisible-large"></div>
  {% endif %}
  {% endif %}

    <main class="main-content">

      {% unless template contains "contact" or template contains "index" or template contains "about" %}
      <div class="dt-sc-hr-invisible-large"></div>
      <div class="wrapper">
        {% endunless %}
        <div class="grid__item">
          {{ content_for_layout }}
        </div>
        {% unless template contains "contact" or template contains "index" or template contains "about" %}
      </div>
      {% endunless %}
      {% unless template contains "about" or template contains "index" or template contains "contact" %}
      <div class="dt-sc-hr-invisible-large"></div>
      {% endunless %}
    </main>


  {% section 'footer-model-3' %}


  {{ 'timber.js' | asset_url | script_tag }}
  {{ 'core-files.js' | asset_url | script_tag }}
  {{ 'theme.js' | asset_url | script_tag }}
  {{ 'option_selection.js' | shopify_asset_url | script_tag }}
  {{ 'api.jquery.js' | shopify_asset_url | script_tag }}
  {{ 'shop.js' | asset_url | script_tag }}
  {{ 'classie.js' | asset_url | script_tag }}
  {% if template contains 'article' or template contains 'product' %}
  {% if settings.social_sharing_blog or settings.social_sharing_products %}
  {{ 'social-buttons.js' | asset_url | script_tag }}
  {% endif %}
  {% endif %}


  {% include 'qmodal' %}
  {% include 'modal-box' %}
  {% include 'currencies' %}
  {% include 'newsletter' %}
  {% if template contains 'index' %}
  {{ 'wow.js' | asset_url | script_tag }}
  {% endif %}



 <script type="text/javascript">
    $('.quick-view .close-window').click(function() {
	$('.quick-view').switchClass("open-in","open-out");
	});
</script>
</body>
</html>
